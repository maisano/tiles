{
  "name": "tiles",
  "version": "0.0.1",
  "description": "n-puzzle game",
  "dependencies": {
    "classnames": "^2.2.5",
    "concurrently": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "http-server": "^0.9.0",
    "modular-css": "^0.27.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "rollup": "^0.34.13",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-eslint": "^2.0.2",
    "rollup-plugin-inject": "^1.4.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  },
  "scripts": {
    "prebuild": "npm run build:worker && cp static/* build/",
    "build:prod": "npm run prebuild && rollup -c config/production.js",
    "build:dev": "npm run prebuild && rollup --watch -c config/development.js",
    "build:worker": "rollup -c config/worker.js",
    "server": "http-server build/",
    "start": "concurrently --kill-others 'npm run build:dev' 'npm run server'"
  },
  "author": "maisano",
  "license": "ISC",
  "devDependencies": {}
}
